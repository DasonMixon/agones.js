{
  "name": "agones.js",
  "version": "1.0.0",
  "description": "Agones server interaction library",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "types": "lib/**/*.d.ts",
  "scripts": {
    "build": "tslint -c tslint.json -p tsconfig.json --fix && tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DasonMixon/agones.js.git"
  },
  "author": "Mason Dixon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DasonMixon/agones.js/issues"
  },
  "homepage": "https://github.com/DasonMixon/agones.js#readme",
  "dependencies": {
    "@kubernetes/client-node": "^0.14.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.9",
    "jasmine": "^3.7.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}